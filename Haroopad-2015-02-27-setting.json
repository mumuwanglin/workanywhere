{
  "Code": {
    "theme": "solarized_light",
    "displayLineNumber": false
  },
  "Editor": {
    "theme": "default",
    "displayLineNumber": false,
    "vimKeyBinding": true,
    "autoPairCharacters": false,
    "useMarkdownFolding": false,
    "indentWithTabs": true,
    "displayActiveLine": true,
    "fontSize": 14,
    "userTheme": "default"
  },
  "General": {
    "enableSyncScroll": true,
    "playKeypressSound": false,
    "enableAutoComplete": false,
    "enableLastFileRestore": false,
    "displayLanguage": "zh-cn",
    "dateFormat": "LLL"
  },
  "Markdown": {
    "tables": true,
    "footnotes": false,
    "breaks": true,
    "sanitize": false,
    "smartLists": true,
    "smartypants": true,
    "mathjax": false,
    "dollarSign": false
  },
  "Viewer": {
    "theme": "haroopad",
    "clickableLink": true,
    "fontSize": 15,
    "userTheme": "default"
  },
  "_version": "0.13.0"
}